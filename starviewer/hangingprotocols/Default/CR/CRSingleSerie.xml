<?xml version="1.0" encoding="UTF-8"?>
<!--
   Aquest Hanging Protocol est pensat per aplicar-se sobre
   estudis de CR que tenen una sola sèrie amb més d'una imatge.
   Al primer visor hi anirà la primera imatge i al segon visor la
   segona imatge
-->
<hangingProtocol name="CRSingleSerie">
<hangingProtocolName>CRSingle</hangingProtocolName>
<numberScreens>1</numberScreens>
<protocol>CR</protocol>
<imageSet identifier="1">
</imageSet>
<displaySet identifier="1">
<imageSetNumber>1</imageSetNumber>
<position>0.0\1.0\0.5\0.0</position>
<imageNumber>0</imageNumber>
</displaySet>
<displaySet identifier="2">
<imageSetNumber>1</imageSetNumber>
<position>0.5\1.0\1.0\0.0</position>
<imageNumber>1</imageNumber>
</displaySet>
</hangingProtocol>